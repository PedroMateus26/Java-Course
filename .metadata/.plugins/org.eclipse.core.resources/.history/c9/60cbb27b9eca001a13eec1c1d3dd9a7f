package application;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number of products: ");
		int n = sc.nextInt();
		for(int i =0; i<n; i++){
			System.out.println("Product #"+i+" data: ");
		}
		sc.close();
	}

}
